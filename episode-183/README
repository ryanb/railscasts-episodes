Commands

  sudo gem update --system
  sudo gem install gemcutter
  gem tumble
  gem build uniquify.gemspec
  gem push uniquify-0.1.0.gem
  sudo gem install jeweler
  rake --tasks
  rake version:write
  rake version:bump:minor
  rake gemcutter:release
